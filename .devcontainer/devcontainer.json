{
	"name": "Haiku Codespace",
	"dockerComposeFile": "./docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"extensions": [
				"svelte.svelte-vscode",
				"golang.go"
			]
		},
		"codespaces": {
			"repositories": {
				"mi759/haikuapi": {
					"permissions": {
						"contents": "write",
						"pull_requests": "write"
					}
				},
				"mi759/haikufrontend": {
					"permissions": {
						"contents": "write",
						"pull_requests": "write"
					}
				}
			}
		}
	}
}